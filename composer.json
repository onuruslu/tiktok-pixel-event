{
    "name": "onuruslu/tiktok-pixel-event-notifier",
    "description": "It sends events to Tiktok.",
    "keywords": [
        "onuruslu",
        "tiktok-pixel-event-notifier"
    ],
    "homepage": "https://github.com/onuruslu/tiktok-pixel-event-notifier",
    "license": "MIT",
    "authors": [
        {
            "name": "Onur Uslu",
            "email": "onrslu@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Onuruslu\\TiktokPixelEventNotifier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Onuruslu\\TiktokPixelEventNotifier\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
